{
  "manifest_version": "ext-v1",
  "name": "TLDraw - a very good whiteboard",
  "version": "1.0.0",
  "icons": {
    "128": "./assets/128.png",
    "128-dark": "./assets/128-dark.png",
    "banner": "./assets/banner.png",
    "banner-dark": "./assets/banner-dark.png",
    "background": "./assets/background.png",
    "background-dark": "./assets/background-dark.png"
  },
  "scripts": ["main.js"],
  "modules": {
    "console": {},
    "runtime": {
      "permissions": ["reload"]
    },
    "tabs": {},
    "timers": {},
    "windows": {},
    "webviews": {},
    "websessions": {
      "module_version": "1.0.1",
      "permissions": ["*"]
    }
  }
}
